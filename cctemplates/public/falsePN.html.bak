<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="otherStyles.css" rel="stylesheet" />
    <title>Document</title>
  </head>
  <body>
    <div id="mySidebar" class="sidebar">
      <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">×</a>
      <p class="divider">Case Comment Templates</p>
      <a href="internal.html">Internal Notes</a>
      <a href="callNotes.html">Call Notes</a>
      <a href="reqhelp.html">Request Help</a>
      <a href="ham.html">Help-A-Moose</a>
      <a href="handoff.html">Global Handoff</a>
      <a href="solved.html">Issue Solved</a>
      <p class="divider">SI Templates</p>
      <a href="falsePN.html">False +/-</a>
      <a href="agentFalse.html">Agent F+/-</a>
      <p class="divider">GraphQL Templates</p>
      <a href="agent.html">Agent</a>
  </div>
    <div id="main">
      <button class="openbtn" onclick="openNav()">☰</button>
    </div>
    <div class="mainBody">
      <div class="boxText">
        <textarea
          id="descriptionSI"
          cols="100"
          rows="10"
          required="required"
        ></textarea>
        <span>Description</span>
      </div>
      <div class="boxText">
        <textarea
          id="engineInfo"
          cols="100"
          rows="3"
          required="required"
        ></textarea>
        <span>Engine Info</span>
      </div>
      <div class="boxText">
        <textarea
          id="asset"
          cols="100"
          rows="1"
          required="required"
        ></textarea>
        <span>Asset</span>
      </div>

      <div class="boxText">
        <textarea
          id="initAuth"
          cols="100"
          rows="5"
          required="required"
        ></textarea>
        <span>Initial Authentication</span>
      </div>

      <div class="boxText">
        <textarea
          id="elevatedperm"
          cols="100"
          rows="5"
          required="required"
        ></textarea>
        <span>Elevated Permissions</span>
      </div>

      <div class="boxText">
        <textarea
          id="systemfingerprint"
          cols="100"
          rows="5"
          required="required"
        ></textarea>
        <span>System Fingerprint (1.0)</span>
      </div>

      <div class="boxText">
        <textarea
          id="vulnfindings"
          cols="100"
          rows="5"
          required="required"
        ></textarea>
        <span>Vulnerability Finding (scan log)</span>
      </div>

      <div class="boxText">
        <textarea
          id="xmlreport"
          cols="100"
          rows="5"
          required="required"
        ></textarea>
        <span>XML Report Proof</span>
      </div>

      <div class="boxText">
        <textarea
          id="farnsworth"
          cols="100"
          rows="3"
          required="required"
        ></textarea>
        <span>Farnsworth</span>
      </div>

      <div class="boxText">
        <textarea
          id="disputeProof"
          cols="100"
          rows="10"
          required="required"
        ></textarea>
        <span>Proof Disputing Finding</span>
      </div>
      <div class="tooltip">
        <button onclick="myFunction()" onmouseout="outFunc()" class="button">
          <span class="tooltiptext" id="myTooltip">Copy to clipboard</span>
          Copy text
        </button>
      </div>
    </div>
    <script src="falseScript.js"></script>
  </body>
</html>
