<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>ccTemplates - Agent False +/-</title>
  <link href="otherStyles.css" rel="stylesheet" />
  <script src="nav.js" defer></script>
  <script src="agentFalseScript.js" defer></script>
</head>
<body>
  <div id="mySidebar" class="sidebar"></div>
  <div id="main"><button class="openbtn" onclick="openNav()">â˜°</button></div>

  <div class="mainBody">
    <h2>Agent False Positive/Negative Template</h2>

    <!-- Standard Fields -->
    <div class="boxText"><textarea id="orgID" cols="100" rows="1"></textarea><span>Org ID</span></div>
    <div class="boxText"><textarea id="agentID" cols="100" rows="1"></textarea><span>Agent ID</span></div>
    <div class="boxText"><textarea id="region" cols="100" rows="1"></textarea><span>Region</span></div>
    <div class="boxText"><textarea id="summary" cols="100" rows="5"></textarea><span>Summary</span></div>
    <div class="boxText"><textarea id="disputeProof" cols="100" rows="5"></textarea><span>Proof Disputing Finding</span></div>
    <div class="boxText"><textarea id="xmlreport" cols="100" rows="3"></textarea><span>Vulnerability Results (Agent Assessment or XML Report Proof)</span></div>
    <div class="boxText"><textarea id="guenter" cols="100" rows="2"></textarea><span>Guenter</span></div>
    <div class="boxText"><textarea id="actionsTakenToTroubleshoot" cols="100" rows="5"></textarea><span>Actions Taken to Troubleshoot</span></div>
    <div class="boxText"><textarea id="expectedBehavior" cols="100" rows="3"></textarea><span>Expected Behavior</span></div>
    <div class="boxText"><textarea id="actualBehavior" cols="100" rows="3"></textarea><span>Actual Behavior</span></div>
    <div class="boxText"><textarea id="requestedOutcomeFromEngineering" cols="100" rows="3"></textarea><span>Requested Outcome from Engineering</span></div>

    <!-- Checkboxes -->
    <h4>Required Attachments</h4>
    <div class="checkbox-container">
      <input type="checkbox" id="agentlogchk"><label for="agentlogchk">Agent Logs</label>
      <input type="checkbox" id="xmlreportchk"><label for="xmlreportchk">XML Report Attached</label>
      <input type="checkbox" id="proofchk"><label for="proofchk">Screenshot or Proof of F+/-</label>
    </div>

    <!-- Buttons -->
    <center>
      <div class="tooltip">
        <button id="copyButton" class="button" onmouseout="outFunc()">
          <span class="tooltiptext" id="myTooltip">Copy to clipboard</span>
          Copy Text
        </button>
        <button id="resetButton" class="button">Reset</button>
      </div>
    </center>
  </div>
</body>
</html>

